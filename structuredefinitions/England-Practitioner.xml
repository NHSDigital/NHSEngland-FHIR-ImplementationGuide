<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="England-Practitioner" />
  <url value="https://fhir.nhs.uk/StructureDefinition/England-Practitioner" />
  <version value="1.0.0" />
  <name value="EnglandPractitioner" />
  <title value="England Practitioner" />
  <status value="draft" />
  <date value="2023-08-18" />
  <publisher value="NHS England" />
  <contact>
    <name value="NHS England" />
    <telecom>
      <system value="email" />
      <value value="interoperabilityteam@nhs.net" />
      <use value="work" />
      <rank value="1" />
    </telecom>
  </contact>
  <description value="This profile defines the NHS England constraints and extensions on the FHIR UK Core resource [UK Core Practitioner](https://simplifier.net/hl7fhirukcorer4/ukcore-practitioner)." />
  <purpose value="This profile allows exchange of information about all individuals who are engaged in the healthcare process and healthcare-related services as part of their formal responsibilities, is used for attribution of activities and responsibilities to these individuals." />
