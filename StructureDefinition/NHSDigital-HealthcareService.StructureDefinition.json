        "path": "HealthcareService.specialty.coding",
        "sliceName": "NHSDataDictionarySpecialty",
        "mustSupport": true,
        "binding": {
          "strength": "required",
          "description": "NHS Data Dictionary Specialty/Treatment Function Code",
